{
  "name": "kuzudb-mcp-server",
  "version": "0.2.4",
  "description": "Kuzu MCP server implementation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "kuzudb-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm run lint && pnpm run typecheck && pnpm run format:check && pnpm run test:coverage && pnpm run build",
    "clean:test-dbs": "node scripts/clean-test-dbs.js"
  },
  "author": "Jordan Burke",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordanburke/kuzudb-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/jordanburke/kuzudb-mcp-server/issues"
  },
  "homepage": "https://github.com/jordanburke/kuzudb-mcp-server#readme",
  "keywords": [
    "mcp",
    "kuzu",
    "graph",
    "database",
    "cypher"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.0",
    "kuzu": "0.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^22.15.32",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "c8": "^10.1.3",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
